{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 0,
        "quotes": [
            "error",
            "backtick"
        ]
    },
    "globals": {
        "document": false
    }
}